<%- include("partials/header"); -%>

<h1>Compose new blog post</h1>
<form class="" action="/compose" method="post">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle" required/>
        <label>Post</label>
        <textarea class="form-control" name="postBody" cols="30" rows="5" required></textarea>
    </div>
    <button class="" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
